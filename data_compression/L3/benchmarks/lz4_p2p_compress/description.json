{
    "name": "LZ4_P2P Application for Compression", 
    "description": [
        "LZ4_P2P application for Compression resides in `L3/benchmarks/lz4_p2p_compress` directory."
    ],
    "flow": "vitis",
    "platform_blacklist": [
        "u50",
        "u280"
    ],
    "platform_whitelist": [
        "samsung_u2",
        "samsung_U2"
    ],
    "launch": [
        {
            "cmd_args": "-cx BUILD/compress.xclbin -p2p 1 -l PROJECT/test.list", 
            "name": "generic launch for all flows"
        }
    ], 
    "match_makefile": "false",
    "config_make": "config.mk",
    "host": {
        "host_exe": "xil_lz4", 
        "compiler": {
            "sources": [
                "./src/host.cpp",
                "LIB_DIR/L3/src/lz4_p2p_comp.cpp",
                "LIB_DIR/common/libs/xcl2/xcl2.cpp",
                "LIB_DIR/common/libs/cmdparser/cmdlineparser.cpp",
                "LIB_DIR/common/libs/logger/logger.cpp",
                "LIB_DIR/common/thirdParty/xxhash/xxhash.c"
            ], 
            "includepaths": [
                "LIB_DIR/L3/include",
                "LIB_DIR/L1/include/hw"
            ], 
            "symbols": [ "PARALLEL_BLOCK=8" , "ENABLE_P2P=1"] 
        }
    },
    "v++":{
        "compiler": {
            "symbols": [
                "PARALLEL_BLOCK=8"
            ]
        }
    }, 
    "containers": [
        {
            "name": "compress",
            "ldclflags": "--profile_kernel data:all:all:all",
            "accelerators": [
                {
                    "name": "xilLz4Compress", 
                    "location": "LIB_DIR/L2/src/lz4_compress_mm.cpp",
                    "num_compute_units": "2",
                    "compute_units" : [
                        {
                            "arguments" : [
                                {
                                    "name":"m_axi_gmem0",
                                    "memory":"bank0"
                                },
                                {
                                    "name":"m_axi_gmem1",
                                    "memory":"bank0"
                                }
                            ]
                        },
                        {
                            "arguments" : [
				{
                                    "name":"m_axi_gmem0",
                                    "memory":"bank0"
                                },
                                {
                                    "name":"m_axi_gmem1",
                                    "memory":"bank0"
                                }
                            ]
                        }
                    ]
                },
		{
                    "name": "xilLz4Packer", 
                    "location": "LIB_DIR/L2/src/lz4_packer_mm.cpp",
                    "num_compute_units": "2",
                    "compute_units" : [
                        {
                            "arguments" : [
                                {
                                    "name":"m_axi_gmem0",
                                    "memory":"bank0"
                                },
                                {
                                    "name":"m_axi_gmem1",
                                    "memory":"bank0"
                                }
                            ]
                        },
                        {
                            "arguments" : [
				{
                                    "name":"m_axi_gmem0",
                                    "memory":"bank0"
                                },
                                {
                                    "name":"m_axi_gmem1",
                                    "memory":"bank0"
                                }
                            ]
                        }
                    ]
                }
            ] 
        }
    ],
    "output_files" : "sample.txt.*",
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    } 
}
