{
    "name": "Xilinx GqeJoin Test",
    "description": "Xilinx GqeJoin Test",
    "flow": "vitis",
    "platform_type": "pcie",
    "platform_whitelist": [
        "u280"
    ],
    "platform_blacklist": [
        "other"
    ],
    "platform_properties": {
        "u280": {
            "host": {
                "compiler": {
                    "symbols": [
                        "USE_HBM"
                    ]
                }
            },
            "v++": {
                "compiler": {
                    "cflags": [
                        "--config PROJECT/conn_u280.ini"
                    ]
                }
            }
        }
    },
    "launch": [
        {
            "name": "generic launch for all flows",
            "cmd_args": " -xclbin BUILD/gqe_join.xclbin -in PROJECT/db_data/dat -scale 10"
        },
        {
            "name": "launch for hw_emu flow",
            "target": "hw_emu",
            "cmd_args": " -xclbin BUILD/gqe_join.xclbin -in PROJECT/db_data/dat -scale 10000"
        }
    ],
    "pre_launch": [
        {
            "launch_cmd": "make -C PROJECT/db_data"
        }
    ],
    "host": {
        "host_exe": "test_gqeJoin.exe",
        "compiler": {
            "sources": [
                "host/test_q5simple.cpp",
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ],
            "includepaths": [
                "LIB_DIR/L1/include/hw",
                "LIB_DIR/L3/include/sw",
                "host",
                "LIB_DIR/ext/xcl2"
            ],
            "options": "-O3 "
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/src",
                "LIB_DIR/L1/include/hw",
                "LIB_DIR/L2/include",
                "LIB_DIR/../utils/L1/include"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "LIB_DIR/L2/src/gqe_join.cpp",
                    "frequency": 200,
                    "name": "gqeJoin"
                }
            ],
            "frequency": 200,
            "name": "gqe_join"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 520
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
