{
    "name": "Xilinx Naive Bayes Training", 
    "description": "", 
    "flow": "vitis", 
    "platform_whitelist": [
        "u200"
    ], 
    "platform_blacklist": [], 
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/naiveBayesTrain_kernel.xclbin -in REPO_DIR/L1/benchmarks/naive_bayes/dat/test.dat -g REPO_DIR/L1/benchmarks/naive_bayes/dat/test_g.dat -c 10 -t 13107",
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "test_nb.exe", 
        "compiler": {
            "sources": [
                "REPO_DIR/L1/benchmarks/naive_bayes/host/test_nb.cpp", 
                "REPO_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/L1/include", 
                "REPO_DIR/L1/benchmarks/naive_bayes/host", 
                "REPO_DIR/L1/benchmarks/naive_bayes/kernel", 
                "REPO_DIR/ext/xcl2"
            ], 
            "options": "-O3 "
        }
    }, 
    "v++": {
        "compiler": {
            "includepath": [
                "REPO_DIR/L1/include", 
                "REPO_DIR/L1/benchmarks/naive_bayes/kernel" 
            ]
        }, 
        "linker": {
            "includepath": [
                "REPO_DIR/L1/include", 
                "REPO_DIR/L1/benchmarks/naive_bayes/kernel" 
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "REPO_DIR/L1/benchmarks/naive_bayes/kernel/naiveBayesTrain_kernel.cpp", 
                    "frequency": 300.0, 
                    "clflags": " -D KERNEL_NAME=naiveBayesTrain_kernel", 
                    "name": "naiveBayesTrain_kernel"
                }
            ], 
            "frequency": 300.0, 
            "name": "naiveBayesTrain_kernel"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 32768, 
                "max_time_min": 300
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
