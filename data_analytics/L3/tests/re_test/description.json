{
    "name": "Xilinx Regex L3 API Test", 
    "description": "Xilinx Regex L3 API Test", 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "u200", 
        "u250", 
        "u50"
    ], 
    "platform_blacklist": [
        "zc"
    ], 
    "platform_properties": {
        "u200": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": []
                }
            }, 
            "containers": [
                {
                    "ldclflags": "--config PROJECT/conn_u200.cfg", 
                    "name": "reEngineKernel"
                }
            ]
        }, 
        "u250": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": []
                }
            }, 
            "containers": [
                {
                    "ldclflags": "--config PROJECT/conn_u250.cfg", 
                    "name": "reEngineKernel"
                }
            ]
        }, 
        "u50": {
            "host": {
                "compiler": {
                    "symbols": []
                }
            }, 
            "v++": {
                "compiler": {
                    "cflags": []
                }
            }, 
            "containers": [
                {
                    "ldclflags": "--config PROJECT/conn_u50.cfg", 
                    "name": "reEngineKernel"
                }
            ]
        }
    }, 
    "launch": [
        {
            "name": "generic launch for all flows", 
            "cmd_args": " -xclbin BUILD/reEngineKernel.xclbin -in LIB_DIR/L2/tests/text/reEngine/log_data/access_5k.log -out PROJECT/check.log",
            "ld_library_path": [
                "PROJECT/re_compile/lib/lib" 
            ]
        }
    ], 
    "pre_build": [
        {
	    "build_cmd": "make -C PROJECT/re_compile"
	}
    ], 
    "gui": false, 
    "host": {
        "host_exe": "test.exe", 
        "compiler": {
            "sources": [
                "PROJECT/host/main.cpp", 
                "LIB_DIR/L3/src/sw/text/regex_engine.cpp", 
                "LIB_DIR/L3/src/sw/text/reEngine_config.cpp", 
                "LIB_DIR/ext/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "PROJECT/kernel", 
                "PROJECT/re_compile/lib/include", 
                "LIB_DIR/L1/include/sw", 
                "LIB_DIR/L3/include/sw", 
                "LIB_DIR/ext/xcl2"
            ], 
            "options": "-O3 "
        }, 
        "linker": {
            "librarypaths": [
                "PROJECT/re_compile/lib/lib"
            ], 
            "libraries": [
                "onig", 
                "xfcompile"
            ], 
            "options": ""
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "PROJECT/kernel",
                "LIB_DIR/L2/include/hw",
                "LIB_DIR/L1/include/hw" 
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "reEngineKernel", 
                    "location": "PROJECT/kernel/re_engine_kernel.cpp", 
                    "frequency": 300, 
                    "num_compute_units": 1
                }
            ], 
            "frequency": 300, 
            "name": "reEngineKernel"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 65536, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 660, 
                    "vitis_hw_emu": 480, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu"
        ], 
        "category": "canary"
    }
}
