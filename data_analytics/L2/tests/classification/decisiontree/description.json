{
    "gui": false,
    "name": "Xilinx Decision Tree Classification Training", 
    "description": "", 
    "flow": "vitis", 
    "platform_whitelist": [
        "u250"
    ], 
    "platform_blacklist": [], 
    "launch": [
        {
            "cmd_args": "-xclbin $(XCLBIN_FILE) -in ml_datasets/ -trn 1000 -ten 100 -fn 26 -cn 2", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
